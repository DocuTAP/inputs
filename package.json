{
  "name": "@docutap-api/inputs",
  "version": "0.0.0-development",
  "homepage": "https://github.com/DocuTAP/inputs",
  "registry": "github",
  "repository": {
    "type": "git",
    "url": "https://github.com/DocuTAP/inputs.git"
  },
  "license": "GPL-3.0",
  "description": "A Vue.js Plugin",
  "author": "Adrian Carriger <acarriger@docutap.com>",
  "main": "dist/inputs.common.js",
  "module": "./@docutap/inputs.esm.js",
  "browser": "dist/inputs.js",
  "unpkg": "dist/inputs.js",
  "style": "dist/inputs.css",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "node build/build.js",
    "build:dll": "webpack --progress --config build/webpack.config.dll.js",
    "lint": "yon run lint:js && yon run lint:css",
    "lint:js": "eslint --ext js --ext jsx --ext vue src test/**/*.spec.js test/*.js build",
    "lint:js:fix": "yon run lint:js -- --fix",
    "lint:css": "stylelint src/**/*.{vue,css}",
    "lint:staged": "lint-staged",
    "pretest": "yon run lint",
    "test": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "dev": "webpack-dashboard -- webpack-dev-server --config build/webpack.config.dev.js --open",
    "dev:coverage": "cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "prepublish": "yon run build"
  },
  "lint-staged": {
    "*.{vue,jsx,js}": [
      "eslint --fix"
    ],
    "*.{vue,css}": [
      "stylefmt",
      "stylelint"
    ]
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@docutap-api/ui": "^1.1.1",
    "add-asset-html-webpack-plugin": "^2.0.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.0",
    "babel-preset-env": "^1.4.0",
    "buble": "^0.15.2",
    "chai": "^3.5.0",
    "chai-dom": "^1.4.0",
    "clean-css": "^4.0.0",
    "condition-circle": "^2.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-vue": "^2.0.0",
    "eslint-plugin-vue": "^2.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.7.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.0",
    "lint-staged": "^3.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "mocha-css": "^1.0.1",
    "postcss": "^6.0.0",
    "postcss-cssnext": "^2.10.0",
    "pre-commit": "^1.2.0",
    "rimraf": "^2.6.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-jsx": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.4.1",
    "rollup-plugin-replace": "^1.1.0",
    "rollup-plugin-vue": "^2.3.0",
    "semantic-commitlint": "^1.3.2",
    "semantic-release": "^15.5.5",
    "sinon": "2.2.0",
    "sinon-chai": "^2.10.0",
    "style-loader": "^0.17.0",
    "stylefmt": "^5.3.0",
    "stylelint": "^7.10.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-html": "^1.0.0",
    "uglify-js": "^3.0.0",
    "uppercamelcase": "^3.0.0",
    "vee-validate": "^2.0.0-rc.17",
    "vue": "^2.3.0",
    "vue-loader": "^12.0.0",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.5.0",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.0",
    "webpack-merge": "^4.0.0",
    "yarn-or-npm": "^2.0.0"
  },
  "peerDependencies": {
    "vue": "^2.3.0"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "style-loader!css-loader!mocha-css",
      "html-entities",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/lib/addStylesClient.js",
      "events",
      "ansi-html",
      "style-loader/addStyles.js"
    ]
  },
  "release": {
    "branch": "develop",
    "verifyConditions": [
      "@semantic-release/git",
      "@semantic-release/github",
      "condition-circle"
    ],
    "getLastRelease": "@semantic-release/git",
    "verifyRelease": [
      "semantic-commitlint"
    ]
  },
  "publishConfig": {
    "tag": "latest",
    "access": "public"
  }
}
